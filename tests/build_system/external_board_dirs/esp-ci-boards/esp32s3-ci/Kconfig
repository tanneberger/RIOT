config BOARD
    default "esp32s3-ci" if BOARD_ESP32S3_CI

config BOARD_ESP32S3_CI
    bool
    default y
    select BOARD_COMMON_ESP32S3
    select CPU_MODEL_ESP32S3_WROOM_1X_N8R8

source "$(RIOTBOARD)/common/esp32s3/Kconfig"
